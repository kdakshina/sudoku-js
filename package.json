{
  "name": "sudoku-js",
  "version": "1.0.0",
  "description": "sudoku solver",
  "main": "lib/index.js",
  "scripts": {
    "start": "node build/bin/index.js",
    "build": "babel lib --out-dir build/bin --source-maps",
    "watch": "babel lib --out-dir build/bin --source-maps --watch",
    "test:build": "babel tests --out-dir build/tests --source-maps",
    "test:watch": "babel tests --out-dir build/tests --source-maps --watch",
    "test": "node build/tests/unit/api.tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdakshina/sudoku-js.git"
  },
  "keywords": [
    "sudoku"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kdakshina/sudoku-js/issues"
  },
  "homepage": "https://github.com/kdakshina/sudoku-js#readme",
  "dependencies": {
    "braces": "^3.0.2",
    "fs": "0.0.1-security",
    "log4js": "^4.4.0",
    "tape": "^4.10.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true,
    "retainLines": true
  }
}
